.df-tabbar {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 9;

  display: flex;
  justify-content: center;
  align-items: center;

  width: 100vw;
  height: 5em;
  margin: 0;
  padding: 0 5vw 2em;

  font-size: 32rpx;
}

.unselected, .selected {
  width: 33%;
  height: 3em;

  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 1000rpx;
}

.unselected {
  background-color: rgb(230, 250, 250);
  color: rgb(84, 184, 172);
}
.selected {
  color: rgb(230, 250, 250);
  background-color: rgb(84, 184, 172);
}
navigator{
  margin: 20rpx;
}

