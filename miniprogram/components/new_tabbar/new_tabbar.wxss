.tab_new{
  position: fixed;
  bottom: 0rpx;
  width: 100vw;
  height: 5em;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tab_elements{
  width: 33.3%;
  height: 3em;
  display: flex;
  justify-content: center;
  align-items: center;
  color: rgb(84, 184, 172);
  font-size: large;
  font-weight: 600;
  background-color: #e6fafa;
  border-radius: 1.5em;
  margin: 10rpx;
}
.selected{
  color: rgb(230, 250, 250);
  background-color: rgb(84, 184, 172);
}