  height: 100vh;
}
.ctext{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: larger;
  font-weight: 600;  
}
.etext{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: medium;
  font-weight: 100;  
}
.btn{
  border-radius: 30rpx !important;
  font-size: large !important;
}
.b{

  margin-top: 20rpx;

  width: 90%;
