Page {
  padding: 25rpx;
  background-color: #e6fafa;
}
.search_area {
  position: relative;
  height: 80rpx;
  margin-bottom: 50rpx;
}
.search_area .search_ipt {
  height: 100%;
  background-color: #f7f7f7;
  padding: 15rpx;
  border-radius: 40rpx;
}
.search_area .search_ico {
  position: absolute;
  top: 15rpx;
  right: 20rpx;
}
.search_content {
  margin-top: 20rpx;
}
.search_content .search_item {
  position: relative;
  height: 150rpx;
  margin: 35rpx 0;
  box-shadow: 5rpx 5rpx 5rpx #aaa;
  border-radius: 20rpx;
  background-color: white;
  padding: 25rpx;
}
.search_content .search_item .ico {
  position: absolute;
  right: 20rpx;
  top: 55rpx;
}
.search_content .search_item .title {
  font-size: larger;
  font-weight: 600;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.search_content .search_item .time {
  margin-top: 15rpx;
  color: #888888;
  font-size: large;
}
