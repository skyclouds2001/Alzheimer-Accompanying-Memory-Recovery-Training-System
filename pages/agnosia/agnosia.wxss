page {
  background-color: rgb(214, 252, 239);
}

#scoreshow{
  position:absolute;
  right: 10rpx;
  top:100rpx;
}
.van-tag {
  font-size: 40rpx !important;
  line-height: 1.25em !important;
}

#tip{
  text-align:center;
  line-height:350rpx ;
  position: absolute;
  font-weight: 700;
  font-size: xx-large;
  width: 350rpx;
  height: 350rpx;
  background-color: rgba(151, 185, 71,0.7);
  color: white;
  border-radius: 20rpx;
  left:200rpx;
  top: 350rpx;
  z-index: 10;
}
#img{
  height: 600rpx;
  margin:200rpx auto;
  margin-bottom: 40rpx;
}
#img image {
  width: 100%;
  height: 100%;
}
#chose {
  height: 300rpx;
  font-weight: 700;
  margin-bottom: 90rpx;
  
}
#submit {
    height: 90rpx;
    width: 650rpx;
    margin-left: 50rpx;
    background-color: rgb(84, 182, 171);
    color: white;
    font-weight: 400;
    border-radius: 30rpx;
    line-height: 90rpx;
}
#final_score{
   position: relative;
   height: 500rpx;
   background-color: rgb(84, 182, 171);
   font-weight: 900;
   color: white;
   margin-top: 300rpx;
  border-radius: 30rpx;
}
#head {
  position: absolute;
  top: 100rpx;
  left: 130rpx;
  font-size: xx-large;
}
#score_show {
  position: absolute;
  top: 300rpx;
  left: 220rpx;
  font-size: large;
}
#restart {
  margin-top: 150rpx;
  height: 110rpx;
  width: 350rpx;
  background-color: rgb(84, 182, 171);
  margin-left: 200rpx;
  border-radius: 25rpx;
  color: white;
  text-align: center;
  line-height: 110rpx;
}
#loadingtip{
  position:absolute;
  left:240rpx;
  top: 450rpx;
}
