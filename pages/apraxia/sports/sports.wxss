page {
  width: 100vw;
  height: 100vh;
}

.image {
  width: 100%;
  height: 560rpx;
  position: relative;
  justify-content: center;
  align-items: center;
  left: 0rpx;
  top: 0rpx;
}

.images {
  top: 0rpx;
  width: 100%;
  height: 560rpx;
  overflow: hidden;
  position: absolute;
}

.content {
  flex-direction: column;
  align-items: center;
  display: flex;
}

.button {
  margin: 20px;
  width: 80%;
  flex-direction: column;
  align-items: center
}

.btn {
  border-radius: 30rpx !important;
}
